{
  "name": "@gisce/react-formiga-table",
  "version": "1.0.0-alpha.2",
  "engines": {
    "node": "20.5.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/react-formiga-table.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-formiga-table.es.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev:vite": "vite build --watch --force",
    "dev:tsc": "tsc --noEmit --watch",
    "prepare": "husky install",
    "start": "concurrently npm:dev:*",
    "check": "lint-staged",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "style-object-to-css-string": "^1.1.3",
    "styled-components": "5.3.5",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@gisce/commitlint-rules": "^1.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.4",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/rollup-plugin-peer-deps-external": "^2.2.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "antd": "^5.5.2",
    "concurrently": "8.2.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "24.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "21.0.7",
    "storybook": "^7.6.3",
    "typescript": "^5.0.4",
    "vite": "4.4.8",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": "rc"
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run build"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
